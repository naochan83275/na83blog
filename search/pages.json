---
permalink: /search/pages.json
---

[
  {%- for path in output_page_paths -%}
    {%- assign my_page = site.pages | where: "path", path | first -%}
    {%- if my_page.title -%}
      {
        "title": "{{ my_page.title | escape }}",
        "url": "{{ my_page.url | relative_url }}"
      }
      {%- if forloop.last == false -%},{%- endif -%}
    {%- endif -%}
  {%- endfor -%}
]
